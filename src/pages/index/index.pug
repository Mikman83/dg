              img(src='./img/Diz_car.svg' alt='Доставка дизеля' title='Доставка дизеля').statistics__img
  .logistics
    form.logistics__advantages.advantages
      input(type="radio" id="gas" name="advantage" value="gas" autocomplete="off" checked)
      label(for="gas").advantages__btn
        span.advantages__btn-gas-img
        span Газ
      input(type="radio" id="diz" name="advantage" autocomplete="off" value="diz")
      label(for="diz").advantages__btn
        span.advantages__btn-diz-img
        span Дизель
    .logistics__delivery.delivery
      .delivery__wrap
        +sort('delivery__sort')
        .delivery__list
          +card('delivery__item', 'text-success', 'text-success')
          +card('delivery__item', 'text-primary', 'text-warning')
          +card('delivery__item', 'text-danger', 'text-success')
          +card('delivery__item', 'text-success', 'text-success')
          +card('delivery__item', 'text-warning', 'text-danger')
          .delivery__promo
            .delivery__promo-wrap
              .delivery__promo-title
                span Готовься к зиме летом!
              .delivery__promo-text
                span Покупайте газ по выгодной цене в низкий сезон, чтобы потом не переплачивать.
          +card('delivery__item', 'text-success', 'text-success')
          +card('delivery__item', 'text-primary', 'text-warning')
          +card('delivery__item', 'text-danger', 'text-success')
          +card('delivery__item', 'text-success', 'text-success')
          +card('delivery__item', 'text-warning', 'text-danger')
        .delivery__show-more
          button.delivery__show-more-btn
            span Показать еще

